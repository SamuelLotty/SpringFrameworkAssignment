type Query {
    getUserByName(username: String!): MyUser
    allUsers: [MyUser!]!
}

type MyUser {
    username: String!
    role: String!
    enabled: Boolean!  
}

input NewUserInput {
    username: String!
    password: String!
    role: String!
}

type Mutation {
    createUser(newUser: NewUserInput!): MyUser!
}
