type Query {
    userById(userID: Int!): MyUser
    allUsers: [MyUser]
}

type MyUser {
    userID: Int
    firstName: String
    password: String
    role: String
    enabled: Boolean
}

input NewUserInput {
    firstName: String!
    password: String!
    role: String!
    enabled: Boolean!

}

type Mutation{
       createUser(newUser: NewUserInput!): MyUser
}